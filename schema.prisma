generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
  age      Int
}

model Todo {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  dont        Boolean
  userId      Int
  time        DateTime @default(now())
}
